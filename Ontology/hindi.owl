    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/parul/ontologies/2018/5/untitled-ontology-35#क्युकम्बर_मोज़ाएक_जीवाणु">
        <rdf:type rdf:resource="http://www.semanticweb.org/saujanya/ontologies/2018/5/untitled-ontology-35#बिमारी"/>
