    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/parul/ontologies/2018/5/untitled-ontology-35#बनाना_मोथ">
        <rdf:type rdf:resource="http://www.semanticweb.org/saujanya/ontologies/2018/5/untitled-ontology-35#बिमारी"/>
